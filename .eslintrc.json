{
  "extends": ["next/core-web-vitals", "airbnb", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".ts", ".tsx"] }],
    "import/extensions": [0],
    "no-param-reassign": [0],
    "react/jsx-props-no-spreading": [0],
    "no-extra-boolean-cast": [1],
    "camelcase": [1],
    "react/require-default-props": [0]
  },
  "globals": {
    "React": true
  }
}
